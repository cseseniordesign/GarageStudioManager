-->
<%= ERB.new(File.read(File.expand_path("#{ROOT}/public/wdn/templates_4.0/includes/scriptsandstyles.html"))).result %>
<!-- TemplateBeginEditable name="doctitle" -->
<title><%= @title %> | University of Nebraska&ndash;Lincoln</title>
<!-- TemplateEndEditable -->
<!-- TemplateBeginEditable name="head" -->
<!-- Place optional header elements here -->

<!-- TemplateEndEditable -->
<!-- TemplateParam name="class" type="text" value="" -->
</head>
<body class="@@(_document['class'])@@" data-version="4.0">
    <%= ERB.new(File.read(File.expand_path("#{ROOT}/public/wdn/templates_4.0/includes/skipnav.html"))).result %>
    <div id="wdn_wrapper">
        <input type="checkbox" id="wdn_menu_toggle" value="Show navigation menu" class="wdn-content-slide wdn-input-driver" />
        <%= ERB.new(File.read(File.expand_path("#{ROOT}/public/wdn/templates_4.0/includes/noscript-padding.html"))).result %>
        <header id="header" role="banner" class="wdn-content-slide wdn-band">
            <%= ERB.new(File.read(File.expand_path("#{ROOT}/public/wdn/templates_4.0/includes/wdnResources.html"))).result %>
            <div class="wdn-inner-wrapper">
                <%= ERB.new(File.read(File.expand_path("#{ROOT}/public/wdn/templates_4.0/includes/logo.html"))).result %>
                <div id="wdn_resources">
                    <%= ERB.new(File.read(File.expand_path("#{ROOT}/public/wdn/templates_4.0/includes/idm.html"))).result %>
                    <%= ERB.new(File.read(File.expand_path("#{ROOT}/public/wdn/templates_4.0/includes/wdnTools.html"))).result %>
                </div>
                <span id="wdn_institution_title">University of Nebraska&ndash;Lincoln</span>
            </div>
            <%=  ERB.new(File.read(File.expand_path("#{ROOT}/public/wdn/templates_4.0/includes/apps.html"))).result %>
            <div class="wdn-inner-wrapper">
                <div id="wdn_site_title">
                    <span><!-- TemplateBeginEditable name="titlegraphic" --><%= @title %><!-- TemplateEndEditable --></span>
                </div>
            </div>
        </header>
        <div id="wdn_navigation_bar" role="navigation" class="wdn-band">
            <nav id="breadcrumbs" class="wdn-inner-wrapper">
                <!-- WDN: see glossary item 'breadcrumbs' -->
                <h3 class="wdn_list_descriptor wdn-text-hidden">Breadcrumbs</h3>
                <!-- TemplateBeginEditable name="breadcrumbs" -->
                <ul>
                    <li><a href="http://www.unl.edu/" title="University of Nebraska&ndash;Lincoln" class="wdn-icon-home">UNL</a></li>
                    <li><a href="#" title="Site Title">Innovation Studio Manager</a></li>
                    <li>Home</li>
                </ul>
                <!-- TemplateEndEditable -->
            </nav>
            <div id="wdn_navigation_wrapper">
                <nav id="navigation" role="navigation" class="wdn-band">
                    <h3 class="wdn_list_descriptor wdn-text-hidden">Navigation</h3>
                    <!-- TemplateBeginEditable name="navlinks" -->
                    <%= ERB.new(File.read(File.expand_path("#{ROOT}/public/wdn/sharedcode/navigation.html"))).result %>
                    <!-- TemplateEndEditable -->
                    <label for="wdn_menu_toggle" class="wdn-icon-menu">Menu</label>
                </nav>
            </div>
        </div>
        <!-- Navigation Trigger -->
        <div class="wdn-menu-trigger wdn-content-slide">
            <label for="wdn_menu_toggle" class="wdn-icon-menu">Menu</label>
        </div>
        <!-- End navigation trigger -->
        <div id="wdn_content_wrapper" role="main" class="wdn-content-slide">
            <div id="maincontent" class="wdn-main">
                <!--THIS IS THE MAIN CONTENT AREA; WDN: see glossary item 'main content area' -->
                <!-- TemplateBeginEditable name="maincontentarea" -->
                <%= yield %>
                <!-- TemplateEndEditable -->
                <!--THIS IS THE END OF THE MAIN CONTENT AREA.-->
            </div>
        </div>
        <div class="wdn-band wdn-content-slide" id="wdn_optional_footer">
            <div class="wdn-inner-wrapper">
                <!-- TemplateBeginEditable name="optionalfooter" -->
                <!-- TemplateEndEditable -->
            </div>
        </div>
        <footer id="footer" role="contentinfo" class="wdn-content-slide">
            <div class="wdn-band" id="wdn_footer_related">
                <div class="wdn-inner-wrapper">
                    <!-- TemplateBeginEditable name="leftcollinks" -->
                    <%= ERB.new(File.read(File.expand_path("#{ROOT}/public/wdn/sharedcode/relatedLinks.html"))).result %>
                    <!-- TemplateEndEditable -->
                </div>
            </div>
            <div class="wdn-band">
                <div class="wdn-inner-wrapper">
                    <div class="footer_col" id="wdn_footer_contact">
                        <h3>Contact Us</h3>
                        <div class="wdn-contact-wrapper">
                            <!-- TemplateBeginEditable name="contactinfo" -->
                            <%= ERB.new(File.read(File.expand_path("#{ROOT}/public/wdn/sharedcode/footerContactInfo.html"))).result %>
                            <!-- TemplateEndEditable -->
                        </div>
                    </div>
                    <div id="wdn_copyright">
                        <div class="wdn-footer-text">
                            <!-- TemplateBeginEditable name="footercontent" -->
                            <%= ERB.new(File.read(File.expand_path("#{ROOT}/public/wdn/sharedcode/footer.html"))).result %>
                            <!-- TemplateEndEditable -->
                            <%= ERB.new(File.read(File.expand_path("#{ROOT}/public/wdn/templates_4.0/includes/wdn.html"))).result %>
                        </div>
                    <%= ERB.new(File.read(File.expand_path("#{ROOT}/public/wdn/templates_4.0/includes/logos.html"))).result %>
                    </div>
                </div>
            </div>
            <%= ERB.new(File.read(File.expand_path("#{ROOT}/public/wdn/templates_4.0/includes/footer_floater.html"))).result %>
        </footer>
        <%= ERB.new(File.read(File.expand_path("#{ROOT}/public/wdn/templates_4.0/includes/noscript.html"))).result %>
    </div>
</body>
</html>
